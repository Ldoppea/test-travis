    "config:base"
  ],
  "masterIssue": true,
  "schedule": "at any time",
  "ignoreDeps": ["^cozy-"],
  "packageRules": [
    {
      "excludePackagePatterns": [
        "^cozy"
      ],
      "rangeStrategy": "pin"
    },
    {
      "matchPackagePatterns": [
        "^cozy"
      ],
      "rangeStrategy": "bump"
    }
  ],
  "timezone": "Europe/Paris",
  "updateNotScheduled": false
}
