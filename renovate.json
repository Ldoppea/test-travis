{
  "extends": [
    "config:base"
  ],
  "masterIssue": true,
  "schedule": "at any time",
  "ignoreDeps": ["cozy-scripts"],
  "packageRules": [
    {
      "excludePackageNames": [
        "cozy-scripts"
      ],
      "rangeStrategy": "pin"
    },
    {
      "matchPackageNames": [
        "^cozy-*"
      ],
      "rangeStrategy": "bump"
    }
  ],
  "timezone": "Europe/Paris",
  "updateNotScheduled": false
}
